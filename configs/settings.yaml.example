default_embedder:
  provider: "openai"
  model: "text-embedding-v3"
  api_key: "sk-..."
  base_url: "https://api.openai.com/v1"
  dimension: 1024

default_llm:
  provider: "openai"
  model: "gpt-4o-mini"
  api_key: "sk-..."
  base_url: "https://api.openai.com/v1"
  temperature: 0.5
  max_tokens: 2000

chat_llm:
  provider: "openai"
  model: "gpt-4o-mini"
  api_key: "sk-..."
  base_url: "https://api.openai.com/v1"
  temperature: 1.0
  max_tokens: 2000

reasoning_llm:
  provider: "openai"
  model: "gpt-4o-mini"
  api_key: "sk-..."
  base_url: "https://api.openai.com/v1"
  temperature: 0.1
  max_tokens: 2000

coding_llm:
  provider: "openai"
  model: "gpt-4o-mini"
  api_key: "sk-..."
  base_url: "https://api.openai.com/v1"
  temperature: 0.1
  max_tokens: 2000

agent_logger:
  level: "INFO"
  format: "%(message)s"
  file: ".fivcadvisor/agent-logs.json"

default_logger:
  level: "INFO"
  format: "[Server] [%(asctime)s] - [%(levelname)s] - %(message)s"
