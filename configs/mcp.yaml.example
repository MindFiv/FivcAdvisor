playwright:
  command: "npx"
  args:
    - "@playwright/mcp@latest"

sequential-thinking:
  command: "npx"
  args:
    - "-y"
    - "@modelcontextprotocol/server-sequential-thinking"

chrome-devtools:
  command: "python"
  args:
    - "/absolute/path/to/chrome-devtools-mcp/server.py"
  env:
    CHROME_DEBUG_PORT: 9222
